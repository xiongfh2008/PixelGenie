{
  "name": "pixelgenie-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node --experimental-modules server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "prebuild": "npm install --prefer-online",
    "build": "tsc && vite build",
    "deploy": "npm install --prefer-online && npm run build && node scripts/create-worker.js && wrangler deploy",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "*",
    "@google/generative-ai": "^0.24.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "rollup": "^4.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "wrangler": "^4.50.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0",
    "@rollup/rollup-win32-x64-msvc": "^4.24.0",
    "@rollup/rollup-darwin-x64": "^4.24.0",
    "@rollup/rollup-darwin-arm64": "^4.24.0"
  }
}