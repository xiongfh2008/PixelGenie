{
  "name": "pixelgenie-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node --experimental-modules server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "test:cloudflare": "node server/test-cloudflare.js",
    "agree:cloudflare": "node server/agree-cloudflare-model.js",
    "prebuild": "npm install --prefer-online --platform=all",
    "build": "node scripts/build-with-rollup-fix.cjs",
    "deploy": "npm run build && wrangler pages deploy dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "*",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "rollup": "^4.18.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "wrangler": "^4.50.0"
  }
}
